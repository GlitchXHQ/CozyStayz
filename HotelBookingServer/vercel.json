{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/clerk/webhook",
      "dest": "server.js",
      "methods": ["POST"],
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
